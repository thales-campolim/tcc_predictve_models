import http.client
import json

conn = http.client.HTTPSConnection("v3.football.api-sports.io")

headers = {
    'x-rapidapi-host': "v3.football.api-sports.io",
    'x-rapidapi-key': "5320a8475eae7538beb73d31f9686385"
}

# Filter by league 71 and season 2025
conn.request("GET", "/teams/statistics?season=2025&team=121&league=71", headers=headers)

res = conn.getresponse()
data = res.read()

print(json.dumps(json.loads(data), indent=4))